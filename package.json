{
  "name": "automationProtractor",
  "version": "0.0.1",
  "author": "sdemianets",
  "homepage": "https://github.com/stasyasin/automationProtractor",
  "private": true,
  "scripts": {
    "lint": "tslint -c tslint.json '**/**/*.ts'",
    "e2e": "ng e2e",
    "e2e:prod": "ng e2e --prod",
    "e2e:dev": "ng e2e --dev-server-target",
    "e2e:watch": "watch 'ng e2e --dev-server-target' src e2e --wait=1",
    "webdriver:start": "webdriver-manager start --seleniumPort 4444",
    "webdriver:update": "webdriver-manager update",
    "protractor": "npx protractor ./protractor.conf.js",
    "sleep": "sleep 10",
    "e2e:run": "concurrently \"npm run webdriver:start\" \"npm run sleep && npm run protractor\" --kill-others",
  },
  "devDependencies": {
    "protractor": "7.0.0",
    "typescript": "4.0.5",
    "xmlhttprequest": "1.8.0",
    "tslint": "6.1.3",
    "@types/jasmine": "3.6.1",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.14.7",
    "@types/selenium-webdriver": "4.0.10",
    "jasmine": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "fs-extra": "4.0.3",
    "eslint": "7.13.0",
    "protractor-html-screenshot-reporter": "0.0.21",
    "protractor-http-mock": "0.9.1",
    "protractor-jasmine2-html-reporter": "0.0.7",
    "require-dir": "0.3.2",
    "run-sequence": "1.2.2",
    "ts-node": "9.0.0",
    "xhr": "2.4.1",
    "moment": "2.29.1",
    "concurrently": "4.0.1",
    "prettier": "1.13.7"
  },
  "description": "Automation Protractor framework",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "bracketSpacing": true
  }
}
